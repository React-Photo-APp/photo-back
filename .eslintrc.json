{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
      "comma-dangle": ["error", "never"],
      "import/prefer-default-export": "off",
      "no-console": "off"
    }
}
